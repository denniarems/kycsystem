<div class="container">
  <h2>KYC Registration</h2>

  <form >
    <div class="form-group">
      <div class="row">
      <label class="col-25" for="publicKey">Public Key:</label>
      <input type="text" class="col-75 form-control" id="publicKeyOnUserForm" placeholder="" readonly name="publicKey">
      </div>
    </div>
    <div class="form-group">
      <label for="name">Name:</label>
      <input type="text" class="form-control" id="name" placeholder="Enter name" required name="name">
    </div>
    <div class="form-group">
      <label for="email">Email:</label>
      <input type="email" class="form-control" id="email" placeholder="Enter email" name="email">
    </div>
    <div class="form-group">
      <label for="dob">Date Of Birth:</label>
      <input type="date" class="form-control" id="dob" name="dob">
    </div>
    <div class="form-group">
      <label for="adderss">Adderss</label>
      <textarea class="form-control" name="adderss" id="adderss" rows="3"></textarea>
    </div>
    <div class="form-group">
      <label for="mobile">Phone Number:</label>
      <input type="number" class="form-control" id="mobile" placeholder="Enter Phone Number" name="mobile">
    </div>
    <div class="form-group">
      <label for="aadhar">Aadhar ID:</label>
      <input type="number" class="form-control" id="aadhar" placeholder="Enter Aadhar Id" name="aadhar">
    </div>
     <div class="col-sm-10 ">
    <button class="btn btn-primary" onclick="userData(event)">Submit</button>
    </div>
    </form>
    <br>
    <br>
    <br>
  
</div>
