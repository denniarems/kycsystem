<link rel="stylesheet" href="/stylesheets/user.css">
{{> iinav }}
<div class="container">
  <h2>KYC Registration</h2>

  <form >
    <div class="form-group">
      <div class="row">
      <label class="col-25" for="publicKey">Public Key:</label>
      <input type="text" class="col-75 form-control" id="publicKeyOnUserForm" placeholder="" readonly name="publicKey">
      </div>
    </div>
    <div class="form-group">
      <label for="name">Name:</label>
      <input type="text" class="form-control" id="name" placeholder="Enter name" required name="name">
    </div>
    <div class="form-group">
      <label for="email">Email:</label>
      <input type="email" class="form-control" id="email" placeholder="Enter email" required name="email">
    </div>
    <div class="form-group">
      <label for="dob">Date Of Birth:</label>
      <input type="date" class="form-control" required id="dob" name="dob">
    </div>
    <div class="form-group">
      <label for="adderss">Adderss</label>
      <textarea class="form-control" name="adderss" required id="adderss" rows="3"></textarea>
    </div>
    <div class="form-group">
      <label for="mobile">Phone Number:</label>
      <input type="number" class="form-control" id="mobile" required placeholder="Enter Phone Number" name="mobile">
    </div>
    <div class="form-group">
      <label for="pincode">Pincode</label>
      <input type="number" class="form-control" id="pincode" required placeholder="6 digit PinCode" name="pincode">
    </div>
    <div class="form-group">
      <label for="aadhar">Aadhar ID:</label>
      <input type="number" class="form-control" id="aadhar" required placeholder="Enter 12 digit Aadhar Id" name="aadhar">
    </div>
    <div class="form-group">
      <label for="voter">Voter ID:</label>
      <input type="number" class="form-control" id="voter" required placeholder="Enter 10 digit Voter Id" name="Voter">
    </div>
    <div class="form-group">
      <label for="enKey">Encryption Key:</label>
      <input type="password" class="form-control" id="enKey" required placeholder="Enter Encryption Key" name="enKey">
    </div>
     <div class="col-sm-10 ">
    <button class="btn btn-primary" onclick="postUserData(event)">Submit</button>
    </div>
    </form>
    <br>
    <br>
    <br>
  
</div>
{{> footer }}
<script src="/javascripts/user.js"></script>